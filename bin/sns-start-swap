#!/usr/bin/env bash
set -euo pipefail

. .demo-env

$ND_DIR/scripts/sns/swap/start_swap --title "$USER $(date +'%Y%m%dT%H%M')" --proposer "$PROPOSER_NEURON_ID" | tee ,start-swap.idl

PROPOSAL_ID="$(<,start-swap.idl idl2json | jq -r '.command[0].MakeProposal.proposal_id[0].id')"
echo "$(./bin/dfx-url nns-dapp)/#/proposal/$PROPOSAL_ID"
